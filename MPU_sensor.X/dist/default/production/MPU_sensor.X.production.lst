.................... void i2c_reset() 
.................... { 
....................  
.................... output_high(PIN_B0); 
.................... for (int i = 0; i < 10; i++)  
....................     { //9nth cycle acts as NACK 
....................         output_high(PIN_B1); 
....................         delay_us(5); 
....................          output_low(PIN_B1); 
....................         delay_us(5); 
....................     } 
....................   output_low(PIN_B0); 
....................   delay_us(5); 
....................   output_high(PIN_B1); 
....................   delay_us(5); 
....................   output_high(PIN_B0); 
....................   delay_us(5); 
....................    
....................   output_float(PIN_B1);                           // SCL 
....................   output_float(PIN_B0);                           // SDA 
....................   i2c_init(1); 
....................  
.................... } 
